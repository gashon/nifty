    "helmet": "^6.0.0",
    "http-errors": "~1.6.3",
    "http-status": "^1.5.3",
    "inversify": "6.0.1",
    "inversify-express-utils": "6.4.3",
    "jsonrepair": "3.0.3",
    "jsonwebtoken": "9.0.2",
